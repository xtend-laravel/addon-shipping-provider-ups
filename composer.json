{
    "name": "xtend-laravel/addon-shipping-provider-ups",
    "description": "UPS Shipping Provider for XtendLunar",
    "type": "library",
    "keywords": [
        "laravel",
        "xtendlunar",
        "shipping",
        "provider",
        "ups"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "XtendLunar\\Addons\\ShippingProviderUps\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adam Lee",
            "email": "adam@codelabx.ltd"
        }
    ],
    "require": {
        "php": "^8.1",
        "ptondereau/laravel-ups-api": "dev-master",
        "saloonphp/saloon": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adam-code-labx/Laravel-UPS-Api.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "XtendLunar\\Addons\\ShippingProviderUps\\ShippingProviderUpsProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    }
}
